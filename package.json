{
    "name": "iobroker.matomo",
    "version": "0.0.7",
    "description": "Imports statistical data from Matomo",
    "author": {
        "name": "Dennis Rathjen",
        "email": "dennis.rathjen@outlook.de"
    },
    "homepage": "https://github.com/o0shojo0o/ioBroker.matomo",
    "license": "MIT",
    "keywords": [
        "matomo",
        "web analytics"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/o0shojo0o/ioBroker.matomo"
    },
    "dependencies": {
        "@iobroker/adapter-core": "^2.5.1",
        "axios": "^0.24.0"
    },
    "devDependencies": {
        "@alcalzone/release-script": "^3.4.3",
        "@babel/cli": "^7.16.8",
        "@babel/core": "^7.16.7",
        "@babel/plugin-proposal-decorators": "^7.16.7",
        "@babel/preset-env": "^7.16.8",
        "@babel/preset-typescript": "^7.16.7",
        "@iobroker/adapter-react": "^2.0.14",
        "@iobroker/testing": "^2.5.1",
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "@sentry/browser": "^6.16.1",
        "@types/chai": "^4.3.0",
        "@types/chai-as-promised": "^7.1.4",
        "@types/gulp": "^4.0.9",
        "@types/mocha": "^9.0.0",
        "@types/node": "^17.0.8",
        "@types/proxyquire": "^1.3.28",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "@types/sinon": "^10.0.6",
        "@types/sinon-chai": "^3.2.8",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "chai": "^4.3.4",
        "chai-as-promised": "^7.1.1",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "gulp": "^4.0.2",
        "mocha": "^9.1.3",
        "parcel-bundler": "^1.12.5",
        "prettier": "^2.5.1",
        "proxyquire": "^2.1.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-icons": "^4.3.1",
        "rimraf": "^3.0.2",
        "sinon": "^12.0.1",
        "sinon-chai": "^3.7.0",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.4.0",
        "typescript": "~4.5.4"
    },
    "main": "build/main.js",
    "scripts": {
        "prebuild": "rimraf ./build",
        "build:parcel": "parcel build admin/src/index.tsx -d admin/build",
        "build:ts": "tsc -p tsconfig.build.json",
        "build": "npm run build:ts && npm run build:parcel",
        "watch:parcel": "parcel admin/src/index.tsx -d admin/build --hmr-port 1235",
        "watch:ts": "tsc -p tsconfig.build.json --watch",
        "watch": "npm run watch:ts",
        "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
        "test:package": "mocha test/package --exit",
        "test:unit": "mocha test/unit --exit",
        "test:integration": "mocha test/integration --exit",
        "test": "npm run test:ts && npm run test:package",
        "check": "tsc --noEmit",
        "lint": "eslint --ext .ts,.tsx src/ admin/src/",
        "release": "release-script"
    },
    "bugs": {
        "url": "https://github.com/o0shojo0o/ioBroker.matomo/issues"
    },
    "readmeFilename": "README.md"
}