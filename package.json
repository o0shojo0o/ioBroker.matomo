{
  "name": "iobroker.matomo",
  "version": "0.0.5",
  "description": "Imports statistical data from Matomo",
  "author": {
    "name": "Dennis Rathjen",
    "email": "dennis.rathjen@outlook.de"
  },
  "homepage": "https://github.com/o0shojo0o/ioBroker.matomo",
  "license": "MIT",
  "keywords": [
    "matomo",
    "web analytics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/o0shojo0o/ioBroker.matomo"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.5.0",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^2.1.0",
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.13.0",
    "@iobroker/adapter-react": "^1.6.22",
    "@iobroker/testing": "^2.4.4",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@sentry/browser": "^6.5.0",
    "@types/chai": "^4.2.19",
    "@types/chai-as-promised": "^7.1.4",
    "@types/gulp": "^4.0.8",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.5",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^17.0.12",
    "@types/react-dom": "^17.0.8",
    "@types/sinon": "^10.0.1",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.26.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "gulp": "^4.0.2",
    "mocha": "^9.0.1",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.0",
    "proxyquire": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5"
  },
  "main": "build/main.js",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:parcel": "parcel build admin/src/index.tsx -d admin/build",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts && npm run build:parcel",
    "watch:parcel": "parcel admin/src/index.tsx -d admin/build --hmr-port 1235",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/o0shojo0o/ioBroker.matomo/issues"
  },
  "readmeFilename": "README.md"
}
